<body>
	<nav>
		<ul>
			<a href="/<%=user._id%>">
			<li id="user-avatar" >
				<img src="<%=user.avatar%>">
			</li>
			</a>
			<% if ((user.groupAdmin === true) || (user.groupAdmin)) { %>
			<a href="/admin/<%= user._id %>">
			<li id="admin-avatar">
				<!-- <label for="admin-avatar">Admin Portal</label> -->
				<img src="https://img.icons8.com/clouds/500/000000/employee-card.png">
			</li>
		</a>
		<%}%>
			<a href="/members/">
			<li id="all-members">
				 <!-- <label for="all-members">Members</label> -->
				 <img src="https://img.icons8.com/clouds/500/000000/conference-call.png"></li>
			</a>
			<a href="/payments">
				<li id="make-payment">
					<!-- <label for="make-payment">Payment</label> -->
					<img src="https://img.icons8.com/cotton/64/000000/mobile-payment--v2.png"></li>
			</a>
			<a href="/transactions">
			<li id="all-transactions">
				 <!-- <label for="all-transactions">Settlements</label> -->
				<img src="https://img.icons8.com/officel/320/000000/ledger.png"></li>
			</a>
			<a href="/member/<%=user._id%>/groups">
			<li id="all-groups">
				 <!-- <label for="all-groups">Groups</label> -->
				<img src="https://img.icons8.com/bubbles/500/000000/people-group-bill--v1.png"></li>
				</a>
				<% if (user) { %><a href="/logout">
			<li id="log-out">
				 <!-- <label for="log-out">Logout</label> -->
				<img src="https://img.icons8.com/cotton/320/000000/logout-rounded.png"></li></a>
				<%}%></li>
			</ul>
	</nav>